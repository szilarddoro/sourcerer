{
  "$schema": "https://json.schemastore.org/eslintrc",
  "root": true,
  "extends": ["airbnb-base", "airbnb-typescript/base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "project": "./apps/cli/tsconfig.json"
  },
  "rules": {
    "operator-linebreak": "off",
    "implicit-arrow-linebreak": "off",
    "no-console": "off",
    "object-curly-newline": "off"
  }
}
